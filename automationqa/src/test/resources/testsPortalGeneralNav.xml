    </test>
    <test name="Index_Test" preserve-order="true">
        <classes>
            <class name="com.lh.test.IndexTest" />
        </classes>
    </test>
    <test name="SelfRegistration_Test" preserve-order="true">
        <classes>
            <class name="com.lh.test.SelfRegistration" />
        </classes>
    </test>
    <test name="Preferences_Test" preserve-order="true">
        <classes>
            <class name="com.lh.test.Preferences" />
