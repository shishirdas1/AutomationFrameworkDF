    </test>

    <test name="Contact Livehealthier - Test" preserve-order="true">
        <classes>
            <class name="com.lh.test.ContactLivehealthier" />
        </classes>
    </test>

    <test name="Community Test" preserve-order="true">
        <classes>
            <class name="com.lh.test.CommunityTest" />
        </classes>
    </test>

    <test name="Wellness Champions" preserve-order="true">
        <classes>
            <class name="com.lh.test.WellnessChampionTest" />
        </classes>
    </test>



</suite>
