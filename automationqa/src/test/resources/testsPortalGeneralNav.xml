<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose="0" name="EPC Test Automation - Portal General Nav" parallel="false">

    <listeners>
