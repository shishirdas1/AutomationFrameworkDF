<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose="0" name="EPC Test Automation - Other" parallel="false">

    <listeners>
        <listener class-name="com.lh.helper.ExtentReporterNG" />
        <listener class-name="com.lh.utils.TestNGCustom" />
    </listeners>

    <test name="File Service Test" preserve-order="true">
        <classes>
            <class name="com.lh.test.fileservice.FileServiceTest" />
        </classes>
    </test>

    <test name="Biometric Import Setup" preserve-order="true">
        <classes>
            <class name="com.lh.test.BiometricImportSetupTest" />
        </classes>
    </test>

    <test name="Bulk Evaluation - Wellness Bank" preserve-order="true">
        <classes>
            <class name="com.lh.test.BulkEvaluation_WellnessBank" />
        </classes>
    </test>

    <test name="Validic Test" preserve-order="true">
        <classes>
            <class name="com.lh.test.ValidicTest" />
        </classes>
    </test>

</suite>
